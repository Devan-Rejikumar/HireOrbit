{
  "name": "application-service",
  "version": "1.0.0",
  "description": "Application management microservice",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js,json --ignore node_modules --ignore dist --ignore uploads --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.12.0",
    "axios": "^1.12.2",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "groq-sdk": "^0.37.0",
    "inversify": "^7.6.1",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.10.0",
    "pdf-parse": "^1.1.1",
    "prisma": "^6.12.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "shared-dto": "file:../shared-dto",
    "winston": "^3.18.3",
    "winston-loki": "^6.1.3",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.1.0",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
