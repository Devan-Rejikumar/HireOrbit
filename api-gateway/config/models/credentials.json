{
  "$id": "credentials",
  "type": "object",
  "properties": {
    "userId": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "enum": ["jwt", "api-key", "oauth2"]
    },
    "secret": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "expiresAt": {
      "type": "string"
    },
    "createdAt": {
      "type": "string",
      "default": "now"
    },
    "updatedAt": {
      "type": "string",
      "default": "now"
    }
  },
  "required": ["userId", "type", "secret"]
}